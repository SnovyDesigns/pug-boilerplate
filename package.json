{
  "name": "es6-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run clean",
    "start": "webpack-dev-server --content-base src/ --mode development --open --hot",
    "prebuild": "npm run clean",
    "build": "npm-run-all build:*",
    "build:bundle": "NODE_ENV=production webpack -p",
    "build:cleanjs": "rimraf build/index.js build/style.js build/about.js",
    "build:cleaninhtml": "node ./utils/clean-index",
    "clean": "rimraf dist build docs build",
    "project": "npm-run-all project:*",
    "project:clean": "rimraf src",
    "project:main-dir": "mkdir -p src",
    "project:dirs": "mkdir -p src/sass src/app src/images",
    "project:files": "touch src/README.md src/index.html src/sass/main.scss src/app/index.js",
    "git": "npm-run-all git:*",
    "git:init": "git init",
    "git:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.6.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.1.1",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^5.3.1",
    "imagemin-gifsicle": "^5.2.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.1.0",
    "imagemin-svgo": "^6.0.0",
    "img-loader": "^3.0.0",
    "jquery": "^3.3.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.3",
    "postcss-loader": "^2.1.5",
    "pug-html-loader": "^1.1.5",
    "replace-in-file": "^3.4.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  }
